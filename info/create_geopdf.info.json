{
  "name": "create_geopdf",
  "runner": "python",
  "categories": ["PDF", "Download"],
  "params": [
    {
      "type": "VoyagerResults",
      "name": "input_items",
      "required": true
    },
    {
      "type": "StringChoice",
      "name": "map_template",
      "choices": [
        "LETTER_LND.mxd",
        "POWER_POINT.mxd",
        "ANSI_D_LND.mxd",
        "ANSI_E_LND.mxd"
      ]
    },
    {
      "type": "MapView",
      "name": "map_view"
    },
    {
      "type": "String",
      "name": "map_title",
      "value": "My Map"
    },
    {
      "type": "String",
      "name": "map_author",
      "value": "Your Name"
    },
    {
      "type": "StringChoice",
      "name": "attribute_settings",
      "choices": [
        "NONE",
        "LAYERS_ONLY",
        "LAYERS_AND_ATTRIBUTES"
      ],
      "advanced":true,
      "value": "LAYERS_AND_ATTRIBUTES"
    }
  ],
  "display": {
    "en": {
      "display": "Create GeoPDF",
      "description": "Add selected data to a GeoPDF document",
      "helpURL": "https://github.com/voyagersearch/tasks/tree/master/docs#create_geopdf",
      "params": {
        "map_template": {
          "display": "Map Template",
          "description": "pick a map template with surrounds",
          "values": {
            "ANSI_D_LND.mxd": "Small Poster (32\" X 24\")",
            "ANSI_E_LND.mxd": "Large Poster (42\" X 32\")",
            "LETTER_LND.mxd": "Page (8.5\" X 11\")",
            "POWER_POINT.mxd": "Powerpoint"
          }
        },
        "map_view": {
          "display": "Map View",
          "description": "Select the map view extent"
        },
        "attribute_settings": {
          "display": "Attribute Settings",
          "description": "Select Feature Attribute Settings",
          "values": {
            "NONE": "No Layers or Features (TODO - copy from ArcMap)",
            "LAYERS_ONLY": "Layer Information (TODO - copy from ArcMap)",
            "LAYERS_AND_ATTRIBUTES": "Layers and Features (TODO - copy from ArcMap)"
          }
        }
      }
    }
  }
}
