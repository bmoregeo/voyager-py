{
  "name": "create_map_service",
  "runner": "python",
  "categories": ["download"],
  "params": [
    {
      "type": "VoyagerResults",
      "name": "input_items",
      "required": true
    },
    {
      "type": "StringChoice",
      "name": "server_type",
      "choices": [
        "ARCGIS_SERVER",
        "MY_HOSTED_SERVICES"
      ],
      "value": "ARCGIS_SERVER"
    },
    {
      "type": "CatalogPath",
      "name": "server"
    },
    {
      "type": "String",
      "name": "service_name"
    },
    {
      "type": "String",
      "name": "folder_name",
    },
    {
      "type": "String",
      "name": "portal_url",
      "value": "https://www.arcgis.com"
    },
    {
      "type": "String",
      "name": "user_name"
    },
    {
      "type": "String",
      "name": "password"
    }
  ],
  "display":
  {
    "en":
    {
      "display": "Create Map Service",
      "description": "Creates an ArcGIS map service",
      "helpURL": "https://github.com/voyagersearch/tasks/tree/master/docs#create_map_service",
      "params":
      {
        "server_type":
        {
            "display": "The ArcGIS for Server type",
            "description": "The type of server."
        },
		"server":
		{
		    "display": "ArcGIS for Server location",
		    "description": "The path to the ArcGIS Server (i.e. GIS Servers/server name) or the path to the connection file ( .ags ). Not required for MY_HOSTED_SERVICES."
		},
		"service_name":
		{
		    "display": "Service name",
		    "description": "The name can only contain alphanumeric characters and underscores. No spaces or special characters are allowed."
		},
		"folder_name":
		{
		    "display": "Folder name",
		    "description": "The folder name. If the folder does not exist, it will be created. The default folder is the server root level."
		},
		"portal_url":
		{
		    "display": "ArcGIS Online Portal URL",
		    "description": "The URL to the ArcGIS Online portal."
		},
		"user_name":
		{
		    "display": "Username",
		    "description": "Portal for ArcGIS username. Required for MY_HOSTED_SERVICES."
		},
		"password":
		{
		    "display": "Password",
		    "description": "Portal for ArcGIS password. Required for MY_HOSTED_SERVICES."
		}
      }
    }
  }
}
