{
  "name": "clip_data",
  "runner": "python",
  "categories": ["download"],
  "params": [
    {
      "type": "VoyagerResults",
      "name": "input_items",
      "required": true
    },
    {
      "type": "Projection",
      "name": "output_projection"
    },
    {
      "type": "StringChoice",
      "name": "output_format",
      "value": "FileGDB",
      "choices": [
        "FileGDB",
        "SHP",
        "MPK",
        "LPK"
      ]
    },
    {
      "type": "Geometry",
      "name": "clip_geometry",
      "initWithResultsExtent": true
    }
  ],
  "display": {
    "en": {
      "display": "Clip Data",
      "description": "Clip data within the clip region; multiple output formats are supported",
      "helpURL": "https://github.com/voyagersearch/tasks/tree/master/docs#clip_data",
      "params": {
        "clip_geometry": {
          "display": "Clip Geometry",
          "description": "The geographic extent or polygon used to clip the data. If not specified, the input results are copied."
        },
        "output_projection": {
          "display": "Projection",
          "description": "The output projection."
        },
        "output_format": {
          "display": "Output Format",
          "description": "The output format."
        }
      }
    },
    "es": {
      "display": "Recortar Datos",
      "description": "Toma de datos de entrada y el clip a varios formatos (FileGDB, etc)",
      "helpURL": "https://github.com/voyagersearch/tasks/tree/master/docs#clip_data_es",
      "params": {
        "clip_geometry": {
          "display": "Geometría del clip",
          "description": "Seleccione una región para recortar"
        }
      }
    }
  }
}
