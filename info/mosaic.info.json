{
  "name": "mosaic",
  "runner": "python",
  "categories": ["download"],
  "params": [
    {
      "type": "VoyagerResults",
      "name": "input_items",
      "required": true
    },
     {
      "type": "Geometry",
      "name": "processing_extent",
      "extentParam": true,
      "initWithItemExtent": true
    },
    {
      "type": "Projection",
      "name": "output_projection"
    },
    {
      "type": "StringChoice",
      "name": "raster_format",
      "choices": [
        "BIP",
        "BMP",
        "BSQ",
        "DAT",
        "GIF",
        "IMG",
        "JPG",
        "JP2",
        "PNG",
        "TIF",
        "GRID",
        "FileGDB"
      ],
  	  "value": "FileGDB"
    },
    {
      "type": "StringChoice",
      "name": "compression_method",
      "choices": [
        "LZ77",
        "JPEG",
        "JPEG2000",
		"PackBits",
        "LZW",
        "RLE",
        "CCITT_G3",
        "CCITT_G4",
        "CCITT_1D",
        "JPEG_YCbCr",
        "NONE"
      ],
      "advanced":true,
      "value": "LZ77"
    },
    {
      "type": "Integer",
      "name": "compression_quality",
      "advanced":true,
      "value": 75
    }
  ],
  "display":
  {
    "en":
	{
      "display": "Mosaic",
      "description": "Mosaic input rasters to a new raster dataset.",
      "helpURL": "https://github.com/voyagersearch/tasks/tree/master/docs#mosaic",
	  "params":
	  {
		"processing_extent":
		{
			"display": "Clip Region",
			"description": "Select the clip region"
		},
		"output_projection":
		{
			"display": "Projection",
			"description": "The projection factory code for setting the output coordinate system"
		},
		"raster_type":
		{
			"display": "Output Raster Type",
			"description": "The type of raster to create."
		},
		"compression_method":
		{
			"display": "Compression Method",
			"description": "choose compression method"
		},
		"compression_quality":
		{
		    "display": "Compression Quality",
		    "description": "compression quality (1 - 100). Default is 75."
		}
	    }
	 }
  }
}
